<!DOCTYPE html>
<html>
  <head>
    <title>HackExchange</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>

    <script type="text/javascript">
      window.images = {
        logoIcon: "<%= asset_path('logo-small-white-green.png') %>",
        logoIconClicked: "<%= asset_path('logo-small-black-green.png') %>",
        logoIconLarge: "<%= asset_path('logo-1.png') %>",
      }
    </script>
  </head>
  <body>
    <%= yield %>
  </body>
</html>
